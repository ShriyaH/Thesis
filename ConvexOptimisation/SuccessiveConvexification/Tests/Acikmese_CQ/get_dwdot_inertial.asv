function [dwdot] = get_dwdot_inertial(w,J,r,ns)

dwdot = zeros(3,ns);

%wrt 
d1 =[0,((J(3,3)-J(2,2))/J(1,1))*w(3),((J(3,3)-J(2,2))/J(1,1))*w(2);
    ((J(1,1)-J(3,3))/J(2,2))*w(3),0,((J(1,1)-J(3,3))/J(2,2))*w(1);
    ((J(2,2)-J(1,1))/J(3,3))*w(2),((J(2,2)-J(1,1))/J(3,3))*w(1),0];

d2 = [0, r(3)/J(1,1), -r(2)/J(1,1);
     -r(3)/J(2,2), 0, r(1)/J(2,2);
      r(2)/J(3,3), -r(1)/J(3,3), 0]; 
  
dwdot(1:3,12:14) = d1;
dwdot(1:3,15:17) = d2;

end